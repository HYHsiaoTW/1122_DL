#!/bin/bash
## make this file as a history record

target_path=/media/sf_D_DRIVE/1122_DeepLearning/Final/preprocess
source_path=$target_path/errts

3dLME \
-prefix $target_path/group+analysis/group+conds -jobs 8 \
-mask $source_path/group_mask+given+tlrc.BRIK \
-model "cond_act" \
-ranEff '~1' \
-SS_type 3 \
-num_glt 2 \
-gltLabel 1 'truth-lie' -gltCode 1 'cond_act : 0.5*comp_truth -0.5*comp_lie' \
-gltLabel 2 'comp-coop' -gltCode 2 'cond_act : 0.25*comp_lie 0.25*comp_truth -0.5*coop_truth' \
-dataTable Subj cond_act InputFile \
s11A comp_lie $source_path/s11A/s11A+beta+comp+lie+tlrc'[0]' \
